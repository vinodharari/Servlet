<web-app>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.vinod.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<servlet-class>com.vinod.RegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/Registration</url-pattern>
	</servlet-mapping>
</web-app>